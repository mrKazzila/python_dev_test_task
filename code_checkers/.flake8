[flake8]
max-line-length = 119
max-complexity = 10
inline-quotes = '

exclude =
    ../.venv,
    ../**/migrations/*,
    ../app/media/*,
    ../app/manage.py,

ignore =
    E722,
    D100,
    D101,
    D102,
    D103,
    D104,
    D105,
    D106,
    D107,
    D400,
    I100,
    I201,
    Q003,
